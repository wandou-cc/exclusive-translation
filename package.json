{
	"name": "exclusive-translation",
	"displayName": "Exclusive Translation",
	"description": "一款简单的安装即用的翻译插件并支持接入三方",
	"publisher": "chenchao",
	"version": "1.2.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"keywords": [
		"Google",
		"Baidu",
		"Ali",
		"有道",
		"百度翻译",
		"有道翻译",
		"翻译",
		"Translate",
		"english",
		"chinese",
		"language"
	],
	"repository": "https://github.com/wandou-cc/exclusive-translation",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:exclusive-translation.translation",
		"onCommand:exclusive-translation.replace"
	],
	"main": "./src/extension.js",
	"icon": "icon.png",
	"contributes": {
		"commands": [
			{
				"command": "exclusive-translation.translation",
				"title": "按键翻译"
			},
			{
				"command": "exclusive-translation.replace",
				"title": "替换翻译"
			}
		],
		"keybindings": [
			{
				"command": "exclusive-translation.translation",
				"key": "ctrl+shift+t",
				"mac": "cmd+shift+t",
				"when": "editorHasSelection"
			},{
				"command": "exclusive-translation.replace",
				"key": "ctrl+alt+r",
				"mac": "ctrl+cmd+r",
				"when": "editorHasSelection"
			}
		],
		"configuration": {
			"title": "Exclusive Translation",
			"properties": {
				"exclusive-translation.englishLanguage": {
					"type": "string",
					"default": "zh",
					"markdownDescription": "输入源为英文翻译的语言"
				},
				"exclusive-translation.exceptEnglishLanguage": {
					"type": "string",
					"default": "en",
					"markdownDescription": "输入源为中日韩等语言翻译"
				},
				"exclusive-translation.translationEngine": {
					"type": "string",
					"default": "Google",
					"enum": [
						"Google",
						"网易有道",
						"百度",
						"阿里巴巴"
					],
					"enumDescriptions": [
						"google翻译接口 无需任何配置参数",
						"网易有道翻译引擎 无需任何配置参数",
						"接入于百度翻译开放者平台",
						"接入阿里开放平台进行翻译"
					]
				},
				"exclusive-translation.isHump": {
					"type": "boolean",
					"markdownDescription": "是否启用驼峰分割翻译;比如`enumDescriptions`将被分割为`enum Descriptions`后翻译"
				},
				"exclusive-translation.isAutoCopy": {
					"type": "boolean",
					"default":false,
					"markdownDescription":"开启后默认进行复制到剪贴板"
				},
				"exclusive-translation.baiduAppid": {
					"type": "string",
					"default": "",
					"definitions": "请输入百度翻译的appId"
				},
				"exclusive-translation.baiduPassword": {
					"type": "string",
					"default": "",
					"definitions": "请输入百度翻译的密钥"
				},
				"exclusive-translation.aliAccessKeyId": {
					"type": "string",
					"default": "",
					"definitions": "请输入阿里翻译的accessKeyId"
				},
				"exclusive-translation.aliAccessKeySecret": {
					"type": "string",
					"default": "",
					"definitions": "请输入阿里翻译的aliAccessKeySecret"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"@alicloud/pop-core": "^1.7.10",
		"axios": "^0.24.0",
		"got": "^11.8.3",
		"js-md5": "^0.7.3",
		"prebuild-install": "^7.0.0",
		"qs": "^6.10.1",
		"vsce": "^2.5.1"
	}
}
